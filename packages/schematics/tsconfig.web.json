{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Web Config",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "moduleResolution": "bundler",
    "types": ["vite/client"],
    "declaration": true,
    "noEmit": false
  }
}
